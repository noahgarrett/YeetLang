fn main() -> int {

    a = 0

    while a < 10 {
        if a == 7 {
            printf("HIT THE BREAK STATEMENT\n")
            break
        }

        if a == 4 {
            printf("HIT THE CONTINUE\n")
            a = a + 2
            continue
        }

        printf("Adding 1\n")
        a = a + 1
    }

    printf("a = %i", a)

    return 0
}